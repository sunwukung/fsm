{
  "name": "swk-fsm",
  "version": "0.0.0",
  "description": "finite state machine library",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "build:watch": "babel --watch src --out-dir dist",
    "test": "find ./src ./test -name '*.js' | xargs mocha --opts resources/mocha.opts",
    "test:watch": "npm run test -- -w",
    "report": "istanbul cover node_modules/.bin/_mocha -- --compilers js:babel/register"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:sunwukung/swk-fsm.git"
  },
  "keywords": [
    "fsm",
    "state-machine",
    "finite-state-machine"
  ],
  "author": "Marcus Kielly",
  "license": "ISC",
  "devDependencies": {
    "babel": "^5.6.14",
    "chai": "^3.0.0",
    "istanbul": "^0.3.19",
    "mocha": "^2.3.0",
    "sinon": "^1.16.1"
  }
}
